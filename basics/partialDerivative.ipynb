   "source": [
    "from torch import tensor\n",
    "u = tensor(3., requires_grad=True)\n",
    "v = tensor(4., requires_grad=True)\n",
    " \n",
    "f = u**3 + v**2 + 4*u*v\n",
    " \n",
    "print(u)\n",
    "print(v)\n",
    "print(f)\n",
    " \n",
    "f.backward()\n",
    "print(\"Partial derivative with respect to u: \", u.grad)\n",
    "print(\"Partial derivative with respect to v: \", v.grad)"
   ]
